<!--pricing table section start here-->


<section id="pricing" class="padding-80">



  <div class="pricing-section-1">
    <div class="container">
      <div class="section-heading text-center">
        <h4 class="small section-title"><span><%= t ('pricing.choose_a_plan')%></span></h4>
        <h2 class="large section-title"><%= t ('pricing.pricing_tables')%></h2>
      </div><!--section heading-->
    </div><!--section heading-->
    <div class="container">
      <div class="row">
        <div class="col-md-4">
          <div class="pricing-wrapper wow animated fadeInDown animated" data-wow-delay=".3s" style="visibility: visible; -webkit-animation: fadeInDown 0.3s;">
            <div class="pricing-wrapper-inner">
              <h1><%= t ('pricing.name_1')%></h1>
              <h2><%= t ('pricing.price_1')%><small style="color: #35bdf6"><%= t ('pricing.per_month')%></small></h2>
              <div class="price-desc">
                <p>15 email accounts</p>
                <p>10 Projects</p>
                <p><i class="ion-close"></i></p>
                <p><i class="ion-close"></i></p>
                <p><i class="ion-close"></i></p>
              </div>

              <div class="price-join">
                <a href="#" class="btn btn-lg btn-theme-color"><%= t ('pricing.choose_plan')%></a>
              </div>
            </div>
          </div><!--pricing wrapper-->
        </div><!--price col-->
        <div class="col-md-4">
          <div class="pricing-wrapper price-popular wow animated fadeInUp animated" data-wow-delay=".6s" style="visibility: visible; -webkit-animation: fadeInUp 0.6s;">
            <div class="pricing-wrapper-inner">
              <h1><%= t ('pricing.name_2')%></h1>
              <h2><%= t ('pricing.price_2')%><small><%= t ('pricing.per_month')%></small></h2>
              <div class="price-desc">
                <p>Unlimited email accounts</p>
                <p>Unlimited Projects</p>
                <p>Unlimited Store (3 years)</p>
                <p>Unlimited users</p>
                <p>Full Support</p>
              </div>

              <div class="price-join">
                <a href="#" class="btn btn-lg btn-theme-color"><%= t ('pricing.choose_plan')%></a>
              </div>
            </div>
          </div><!--pricing wrapper-->
        </div><!--price col-->
        <div class="col-md-4">
          <div class="pricing-wrapper wow animated fadeInRight animated" data-wow-delay=".9s" style="visibility: visible; -webkit-animation: fadeInRight 0.9s;">
            <div class="pricing-wrapper-inner">
              <h1><%= t ('pricing.name_3')%></h1>
              <h2><%= t ('pricing.price_3')%><small style="color: #35bdf6"><%= t ('pricing.per_month')%></small></h2>
              <div class="price-desc">
                <p>Unlimited email accounts</p>
                <p>Unlimited Projects</p>
                <p>500 GB Storage</p>
                <p>Unlimited users</p>
                <p>Full Support</p>
              </div>

              <div class="price-join">
                <a href="#" class="btn btn-lg btn-theme-color"><%= t ('pricing.choose_plan')%></a>
              </div>
            </div>
          </div><!--pricing wrapper-->
        </div><!--price col-->
      </div>
    </div>
  </div><!--price section one end here-->


  <div class="pricing-section-2 parallax" data-stellar-background-ratio="0.5">
    <div class="container text-center wow animated bounceIn" style="visibility: hidden; -webkit-animation: none;">
      <h1><%= t ('contact.ideas')%> </h1>
      <p><a href="#contact" class="btn btn-lg btn-theme-color"><%= t ('contact.contact')%></a></p>
    </div>
  </div>

</section> <!--pricing table section end here-->