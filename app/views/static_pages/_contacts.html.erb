<section id="contact" class="padding-80">


  <div class="contact-sec-1">
    <div class="container">
      <div class="section-heading text-center">
        <h4 class="small section-title"><span><%= t ('contact.say_hello')%>  </span></h4>
        <h2 class="large section-title"><%= t ('contact.contact')%></h2>
      </div><!--section heading-->
    </div><!--.container-->
    <div class="container">
      <div class="row">
        <div class="col-md-12" align="center">
          <h4><%= t ('contact.contact_text')%></h4>
          <div class="contact-info wow animated fadeInRight" data-wow-delay=".6s" style="visibility: hidden; -webkit-animation: none 0.6s;">
            <p> <%= t ('contact.company_name')%></p>
            <p><img alt="" src="/assets/map-marker2-black@2x.png" style="width: 30px; height: 30px; margin-top: -4px;"></i> <%= t ('contact.address')%></p>
            <p><img alt="" src="/assets/phone-sign-black@2x.png" style="width: 30px; height: 30px; margin-top: -4px;"> <%= t ('contact.phone')%></p>
            <p><img alt="" src="/assets/mail-black@2x.png" style="width: 30px; height: 30px; margin-top: -4px;"> <a href="#"><%= t ('contact.email')%></a></p>
            <p><img alt="" src="/assets/clock-black@2x.png" style="width: 30px; height: 30px; margin-top: -4px;"> <%= t ('contact.work_hours')%></p>
            <hr>
            <!--<h4>Elsewhere</h4>
            <ul class="list-inline social-colored">
              <li><a href="#"><i class="ion-social-facebook icon-fb" data-toggle="tooltip" title="" data-original-title="Facebook" data-placement="top"></i></a></li>
              <li><a href="#"><i class="ion-social-twitter icon-twit" data-toggle="tooltip" title="" data-original-title="Twitter" data-placement="top"></i></a></li>
              <li><a href="#"><i class="ion-social-googleplus icon-plus" data-toggle="tooltip" title="" data-original-title="Google pluse" data-placement="top"></i></a></li>
              <li><a href="#"><i class="ion-social-linkedin icon-in" data-toggle="tooltip" title="" data-original-title="Linkedin" data-placement="top"></i></a></li>

            </ul> <!--colored social-->
          </div>
        </div>
      </div>
    </div>
  </div><!--Contact-sec-1 end-->

</section><!--contact section end-->